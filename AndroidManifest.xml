<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.com.rafaeldias.IntentServiceExample">
    <uses-sdk android:minSdkVersion="19"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.Holo.Light.DarkActionBar">
        <activity android:name=".IntentActivityExample" android:label="Intent Service Activity Example">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service android:name=".IntentServiceExample" android:exported="false"/>
    </application>
</manifest>
